<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="DeviceId" value="MsmqSensorTask"/>
    <add key="SensorScanningPeriod" value="00:00:00.1"/>
    <add key="ServerUri" value="http://MICHI-MONSTER/GatewayService"/>
    <add key="DeviceGatewayInputQueueAddress" value="FormatName:DIRECT=OS:MICHI-MONSTER\private$\DefaultInputQueue"/>
    <add key="DeviceGatewayOuputQueueAddress" value="FormatName:DIRECT=OS:MICHI-MONSTER\private$\DefaultOutputQueue"/>
    <add key="DeviceGatewayCommHandlerInputQueueAddress" value="FormatName:DIRECT=OS:MICHI-MONSTER\private$\DefaultCommHandlerInputQueue"/>
    <add key="DeviceGatewayCommHandlerOutputQueueAddress" value="FormatName:DIRECT=OS:MICHI-MONSTER\private$\DefaultCommHandlerOutputQueue"/>

    <add key="PerfCounter.ProcessorLoad" value="Processor|% Processor Time|_Total|Calculated"/>
    <add key="PerfCounter.MemoryAvailbleMBytes" value="Memory|Available MBytes||Calculated"/>
    <add key="PerfCounter.SQLTransactionPerSec" value="SQLServer:Databases|Transactions/sec|Experiments|Calculated"/>

    <add key="PerfCounter.CentralServerGetSensorDataLatest" value="Experiment CentralServerService methods|TotalRequests|GetSensorDataLatest|Calculated"/>
    <add key="PerfCounter.CentralServerGetSensorDataLatestCompletionTime" value="Experiment CentralServerService methods|LastRequestCompletionTime|GetSensorDataLatest|Calculated"/>


    <add key="PerfCounter.CentalServerStoreMultipleSensorData" value="Experiment CentralServerService methods|TotalRequests|StoreMultipleSensorData|Calculated"/>
    <add key="PerfCounter.CentralServerStoreSingleSensorData" value="Experiment CentralServerService methods|TotalRequests|StoreSingleSensorData|Calculated"/>
    <add key="PerfCounter.CentralServerGetSensorData" value="Experiment CentralServerService methods|TotalRequests|GetSensorData|Calculated"/>
    <add key="PerfCounter.CentralServerGetNextCorrelationId" value="Experiment CentralServerService methods|TotalRequests|GetNextCorrelationId|Calculated"/>


    <add key="TrackingPoint.PerformTracking" value="true"/>
    <add key="TrackingPoint.ConnectionString" value="Data Source=MICHI-MONSTER;Initial Catalog=Experiments;Persist Security Info=True;User ID=EXPERIMENT;Password=EXPERIMENT;Max Pool Size=100;Pooling=true"/>
  </appSettings>
</configuration>